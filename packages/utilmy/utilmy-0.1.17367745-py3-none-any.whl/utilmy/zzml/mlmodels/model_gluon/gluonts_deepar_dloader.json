{

    "a_deepar_covid": {
         "model_pars": {
             "model_uri"  : "model_gluon.gluonts_model",
             "model_name" : "deepar",
             "model_pars" : {
                 "prediction_length"    : 20,
                 "freq"                 : "D",
                 "distr_output"         : "gluonts.distribution.neg_binomial:NegativeBinomialOutput",
                
                 "use_feat_static_real" : true,
                 
                 "num_layers"           : 2,
                 "num_cells"            : 40,
                 "cell_type"            : "lstm",
                 "dropout_rate"         : 0.1,

                 "time_features"        : "None",
                 "scaling"              : true,
                 "num_parallel_samples" : 100
             }
             
             
        },


        "data_pars": {
            "data_info": {},
            "preprocessors": [
                {   "name" :  "get_dataset_process",
                    "uri"  : "mlmodels.model_gluon.gluonts_model:get_dataset2",
                    "args"  : { 

                        "train"                 : true,
                        "dt_source"             : "",

                        "data_type"             : "gluonts",

                        "use_feat_dynamic_real" : false,
                        "use_feat_static_cat"   : false,
                        "use_feat_static_real"  : true,
                       
                        "submission"            : false ,
                        
                        "data_path"             : "dataset/timeseries/covid19_gluonts/" ,
                       
                        "numseries"             : 1
                    }
                }
            ]
        },




            
        "compute_pars": {
            "num_samples": 100,
            
            "learning_rate"         : 1e-3,
            "epochs"                : 100,
            "num_batches_per_epoch" : 50,
            "batch_size"            : 32,   
            
            "compute_pars" : {
                
                "batch_size"                 : 32, "clip_gradient": 10, "epochs": 50, "learning_rate": 1e-5,
                "learning_rate_decay_factor" : 0.5,
                
                "num_batches_per_epoch"      : 50,
                
                "minimum_learning_rate"      : 5e-05
            }
        },
        
      "out_pars": {
         "path": "ztest/model_gluon/gluonts_deepar/",
         "plot_prob": true, "quantiles": [0.5,0.67]
      }
    },





