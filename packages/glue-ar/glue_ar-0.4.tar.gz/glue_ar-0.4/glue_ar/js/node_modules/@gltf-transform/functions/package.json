		"ndarray-lanczos": "^0.3.0",
		"ndarray-pixels": "^4.1.0"
	},
	"files": [
		"dist/",
		"src/",
		"README.md",
		"LICENSE.md",
		"package.json",
		"package-lock.json"
	],
	"publishConfig": {
		"access": "public"
