<odoo>
<data noupdate="1">
<record id="sequence_assurance_report" model="ir.sequence">
    <field name="name">Assurance Report</field>
    <field name="code">accountant.assurance_report</field>
    <field name="padding">5</field>
    <field name="prefix">AR/%(year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
</record>

<record id="sequence_nonassurance_report" model="ir.sequence">
    <field name="name">Non-Assurance Report</field>
    <field name="code">accountant.nonassurance_report</field>
    <field name="padding">5</field>
    <field name="prefix">NAR/%(year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
</record>
</data>
</odoo>
